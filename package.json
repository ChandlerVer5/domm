{
  "name": "doem",
  "version": "1.0.0",
  "description": "A functional DOM traversal and manipulation library for modern browsers",
  "scripts": {
    "test": "xo && browserify test/*.js | tape-run --render=faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasperisager/doem.git"
  },
  "keywords": [
    "functional",
    "dom",
    "traversal",
    "manipulation",
    "es6",
    "modern",
    "browser"
  ],
  "author": "Kasper Kronborg Isager <kasperisager@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kasperisager/doem/issues"
  },
  "homepage": "https://github.com/kasperisager/doem#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "faucet": "0.0.1",
    "tape": "^4.4.0",
    "tape-run": "^2.1.2",
    "xo": "^0.12.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "xo": {
    "space": true,
    "exnext": true,
    "envs": [
      "browser"
    ]
  }
}
