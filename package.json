{
  "name": "doem",
  "version": "0.1.0",
  "description": "A functional DOM traversal and manipulation library for modern browsers",
  "license": "MIT",
  "repository": "kasperisager/doem",
  "author": "Kasper Kronborg Isager <kasperisager@gmail.com>",
  "scripts": {
    "test": "xo && browserify test/*.js | tape-run",
    "transpile": "babel --presets es2015 lib -d dist",
    "prepublish": "npm run transpile"
  },
  "main": "dist/index.js",
  "jsnext:main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "functional",
    "dom",
    "traversal",
    "manipulation",
    "es6",
    "modern",
    "browser"
  ],
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "tape": "^4.4.0",
    "tape-run": "^2.1.2",
    "xo": "^0.12.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "xo": {
    "space": true,
    "exnext": true,
    "envs": [
      "browser"
    ]
  }
}
