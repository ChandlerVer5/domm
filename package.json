{
  "name": "doem",
  "version": "1.0.0",
  "description": "A functional DOM traversal and manipulation library for modern browsers",
  "license": "MIT",
  "repository": "kasperisager/doem",
  "author": "Kasper Kronborg Isager <kasperisager@gmail.com>",
  "scripts": {
    "test": "xo && browserify test/*.js | tape-run"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "keywords": [
    "functional",
    "dom",
    "traversal",
    "manipulation",
    "es6",
    "modern",
    "browser"
  ],
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "tape": "^4.4.0",
    "tape-run": "^2.1.2",
    "xo": "^0.12.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "xo": {
    "space": true,
    "exnext": true,
    "envs": [
      "browser"
    ]
  }
}
